	"scripts": {
		"start": "NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" node --experimental-specifier-resolution=node ./built/index.js",
		"start:test": "NODE_ENV=test NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" node --experimental-specifier-resolution=node ./built/index.js",
		"migrate": "NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" node --experimental-specifier-resolution=node ./migrate.js",
		"revertmigration": "NODE_OPTIONS=\"--no-warnings --experimental-vm-modules\" node --experimental-specifier-resolution=node ./revert-migration.js",
		"build": "node ../../scripts/run-swc.js src -d built -D",
		"watch": "node ../../scripts/run-swc.js src -d built -D -w",
		"lint": "eslint --quiet \"src/**/*.ts\"",
		"lint:fix": "eslint --fix \"src/**/*.ts\"",
		"lint:all": "eslint \"src/**/*.ts\"",
		"mocha": "cross-env NODE_ENV=test DEBUG=* TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=\"./test/tsconfig.json\" mocha --timeout 10000",
		"test": "cross-env NODE_ENV=test DEBUG=* TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true TS_NODE_PROJECT=\"./test/tsconfig.json\" mocha --timeout 10000",
		"vitest": "NODE_ENV=test vitest run",
		"test:no-db": "NODE_ENV=test vitest run --config vitest-no-db.config.js",
		"test:simple": "NODE_ENV=test node --experimental-vm-modules --experimental-specifier-resolution=node ./test/simple-runner.js"
